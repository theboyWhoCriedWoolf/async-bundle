{
  "name": "async-bundle",
  "version": "0.0.6",
  "description": "Asynchronous code splitting for React, Redux and Redux-Saga",
  "main": "lib/index.js",
  "author": "Tal Woolf <tal@theboywhocriedwoolf.com>",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_ENV=development babel-node devServer.js",
    "compile:min": "uglifyjs dist/asyncBundle.js -mt -o dist/asyncBundle.min.js",
    "compile:umd": "mkdirp dist/ && babel -o dist/asyncBundle.js src/",
    "compile:commonjs": "babel -d lib/ src/",
    "compile": "npm run compile:commonjs && npm run compile:umd && npm run compile:min",
    "lint": "eslint src/**",
    "test-dev": "jest --watchAll",
    "test": "jest",
    "clean": "rimraf lib && rimraf dist",
    "prepublish": "npm run clean && npm run compile"
  },
  "bugs": {
    "url": "https://github.com/theboyWhoCriedWoolf/async-bundle/issues"
  },
  "dependencies": {
    "invariant": "^2.2.2",
    "prop-types": "^15.5.10",
    "ramda": "^0.24.1",
    "warning": "^3.0.0"
  },
  "tags": [
    "react",
    "react router",
    "webpack",
    "async",
    "redux",
    "redux saga",
    "react boilerplate",
    "code splitting"
  ],
  "keywords": [
    "reactjs",
    "react",
    "asynchronous loader",
    "redux",
    "webpack",
    "webpack bundles",
    "bundle splitting",
    "redux-saga",
    "redux",
    "sagas",
    "react router",
    "webpack",
    "bundle loader",
    "babel-plugin-dynamic-import-node",
    "babel import",
    "async",
    "code splitting"
  ],
  "peerDependencies": {
    "react": "^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "bundle-loader": "^0.5.5",
    "css-loader": "^0.28.5",
    "enzyme": "^2.9.1",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.4",
    "fast-async": "^6.3.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "mkdirp": "^0.5.1",
    "react": "^15.0.0",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.2.0",
    "react-router-redux": "next",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-create-reducer": "^1.1.1",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "redux-saga": "^0.15.6",
    "rimraf": "^2.6.2",
    "style-loader": "^0.18.2",
    "uglify-es": "^3.1.1",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^1.12.0",
    "whatwg-fetch": "^2.0.3"
  }
}
